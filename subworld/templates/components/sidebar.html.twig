<h5 class="text-white font-bold text-lg mt-4 mb-2">COMMUNITIES</h5>
<ul class="space-y-2">
    <li><a href="#" class="text-gray-300 hover:text-white">Create a community</a></li>
    {% for subworld in subworlds %}
        <li>
            <a href="{{ path('app_subworld_show', { 'id': subworld.id }) }}" class="text-gray-300 hover:text-white">
                {{ subworld.name }}
            </a>
        </li>
    {% endfor %}
</ul>

